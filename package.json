{
  "name": "@achasoft/n8n-nodes-opskit",
  "version": "0.1.0",
  "description": "Ops primitives for n8n: rate limits, semaphores, circuit breaker, idempotency, retries, DLQ & replayer.",
  "author": "Navid Kianfar",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": { "access": "public" },
  "repository": { "type": "git", "url": "https://github.com/navid-kianfar/OpsKit.git" },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/RateLimitGate.node.js",
      "dist/nodes/Semaphore.node.js",
      "dist/nodes/CircuitBreaker.node.js",
      "dist/nodes/Idempotency.node.js",
      "dist/nodes/BackoffRetry.node.js",
      "dist/nodes/DeadLetter.node.js",
      "dist/nodes/DLQReplayer.node.js"
    ],
    "credentials": [
      "dist/credentials/OpsKitRedisApi.credentials.js"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.9.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.12.0+sha1.7134f9b8d0b70bc7a1954f47af52123cd2b2acce"
}
